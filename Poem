maar gaya mashrood
la la lalllllalaaaa....

class Animal
{
public static void run() {
System.out.println("Animals are running.");
}
public static void eat(){
System.out.println("Animals are eating anything.");
}
}

class Tiger extends Animal {
public static void run(){
System.out.println("Tiger is running with speed of 100KM/Hr.");
}
public static void eat() {
System.out.println("Tiger can eat upto 20 ton.");
}
}

class Client{
  Tiger t = new Tiger();
  t.run();
  t.eat();
}
